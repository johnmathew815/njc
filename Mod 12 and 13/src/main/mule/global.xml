		<http:request-connection host="localhost" port="8081" />
	</http:request-config>
	<vm:config name="VM_Config" doc:name="VM Config" doc:id="4cd747af-f6be-4b15-9141-b7d84736c77e" >
		<vm:queues >
			<vm:queue queueName="goodbye" />
